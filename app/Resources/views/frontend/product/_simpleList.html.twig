{% for product in products.data %}
    <div class="media margin-clear">
        <div class="d-flex pr-2">
            <div class="overlay-container">
                <img class="media-object" src="/assets/frontend/images/product-5.jpg" alt="blog-thumb">
                <a href="shop-product.html" class="overlay-link small"><i class="fa fa-link"></i></a>
            </div>
        </div>
        <div class="media-body">
            <h6 class="media-heading"><a href="shop-product.html">{{ product.name }}</a></h6>
            <p class="margin-clear">
                <i class="fa fa-star text-default"></i>
                <i class="fa fa-star text-default"></i>
                <i class="fa fa-star text-default"></i>
                <i class="fa fa-star text-default"></i>
                <i class="fa fa-star text-default"></i>
            </p>
            <p class="comment-meta">2 à 5 joueurs <br>à partir de 8 ans</p>
        </div>
    </div>
    {% if not loop.last %}
        <hr>
    {% endif %}
{% endfor %}
