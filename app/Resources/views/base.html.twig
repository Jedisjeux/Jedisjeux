<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}Jedisjeux - et les autres jours aussi{% endblock title %}</title>
    <!-- Description, Keywords and Author -->
    <meta name="description" content="Your description">
    <meta name="keywords" content="Your,Keywords">
    <meta name="author" content="ResponsiveWebInc">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    {% block head_style %}
        {% stylesheets
        '@MopaBootstrapBundle/Resources/public/sass/mopabootstrapbundle-3.2.scss'
        'css/font-awesome.min.css'
        'css/magnific-popup.css'
        'css/owl.carousel.css'
        'css/styles/style.css'
        'css/styles/skin-lblue.css'
        'bower_components/jquery-ui/themes/base/jquery-ui.min.css'
        'bower_components/bootstrap-star-rating/css/star-rating.min.css'
        'bower_components/fancybox/source/jquery.fancybox.css'
        'bower_components/fancybox/source/helpers/jquery.fancybox-buttons.css'
        'bower_components/fancybox/source/helpers/jquery.fancybox-thumbs.css'
        'bower_components/multiple-select/multiple-select.css'
        '../app/Resources/sass/base.scss'
        '../app/Resources/sass/module/core/*.scss'
        '../app/Resources/sass/module/collection/*.scss'
        filter='cssrewrite' output='css/compiled/all.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}

    {% endblock head_style %}

    <!-- Styles -->

    <!-- Favicon -->
    <link rel="shortcut icon" href="#">
</head>

<!-- Add class "boxed" along with body for boxed layout. -->
<!-- Add "pattern-x" (1 to 5) for background patterns. -->
<!-- Add "img-x" (1 to 5) for background images. -->
<body>

{% include 'layout/loading.html.twig' %}

<!-- Outer Starts -->
<div class="outer">

    <!-- Top bar starts -->
    {% include "layout/top-bar.html.twig" %}

    <!-- Top bar ends -->

    <!-- Header One Starts -->
    {% include "layout/header-1.html.twig" %}

    <!-- Header one ends -->

    <br/>

    <!-- Main content starts -->

    <div class="main-block">

        <div class="magazine">

            <div class="container">

                <div class="row">

                    {% block fos_user_content %}
                    {% block body %}
                    {% block container %}
                    <!--
                    Inherit this block to write
                    Content without sidebar
                    -->
                    <div class="col-md-8">

                        {% block container_content %}
                            <!--
                            Inherit this block to write
                            Content with a sidebar
                            -->
                        {% endblock container_content %}

                    </div>

                    <!-- Sidebar column -->
                    <div class="col-md-4">

                        <!-- Sidebar -->
                        <div class="sidebar">

                            {% block sidebar_content %}
                                <!--
                                Inherit this block to write
                                Content in the sidebar
                                -->
                            {% endblock sidebar_content %}

                        </div>

                    </div>

                    {% endblock container %}
                    {% endblock body %}
                    {% endblock fos_user_content %}

                </div>

            </div>

        </div>

        <!-- Main content ends -->

        <!-- Foot Starts -->

        {% include "layout/foot.html.twig" %}

        <!-- Foot Ends -->

        <!-- Footer Starts -->

        {% include "layout/footer.html.twig" %}

        <!-- Footer Ends -->

    </div>

    <!-- Outer Ends -->

    <!-- Scroll to top -->
    <span class="totop"><a href="#"><i class="fa fa-angle-up bg-color"></i></a></span>

    <!-- Javascript files -->

    {% block foot_script_assetic %}
        {% javascripts output='js/compiled/all.js'
        'bower_components/jquery/dist/jquery.min.js'
        '@MopaBootstrapBundle/Resources/public/bootstrap-sass/assets/javascripts/bootstrap/tooltip.js'
        '@MopaBootstrapBundle/Resources/public/bootstrap-sass/assets/javascripts/bootstrap/*.js'
        '@MopaBootstrapBundle/Resources/public/js/mopabootstrap-collection.js'
        '@MopaBootstrapBundle/Resources/public/js/mopabootstrap-subnav.js'
        'bower_components/jquery-ui/jquery-ui.min.js'
        'bower_components/bootstrap-star-rating/js/star-rating.min.js'
        'bower_components/fancybox/lib/jquery.mousewheel-3.0.6.pack.js'
        'bower_components/fancybox/source/jquery.fancybox.pack.js'
        'bower_components/fancybox/source/helpers/jquery.fancybox-buttons.js'
        'bower_components/fancybox/source/helpers/jquery.fancybox-media.js'
        'bower_components/fancybox/source/helpers/jquery.fancybox-thumbs.js'
        'bower_components/multiple-select/jquery.multiple.select.js'
        'js/*'
        'js/*/*'
        '@JDJJeuBundle/Resources/public/js/jeu/*' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    {% endblock foot_script_assetic %}

</body>
</html>