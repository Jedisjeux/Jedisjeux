<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}Jedisjeux - et les autres jours aussi{% endblock title %}</title>
    <!-- Description, Keywords and Author -->
    <meta name="description" content="Your description">
    <meta name="keywords" content="Your,Keywords">
    <meta name="author" content="ResponsiveWebInc">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {#'assets/bower_components/jquery-ui/themes/base/jquery-ui.min.css'#}

    {% block head_style %}
        {% include "CmfCreateBundle::includecssfiles.html.twig" %}
        {% stylesheets
        '@MopaBootstrapBundle/Resources/public/sass/mopabootstrapbundle-3.2.scss'
        'bundles/bmatznerfontawesome/css/font-awesome.min.css'
        'assets/css/magnific-popup.css'
        'assets/css/owl.carousel.css'
        'assets/css/styles/style.css'
        'assets/css/styles/skin-lblue.css'
        'assets/stylesheets/base.css'
        'bundles/bmatznerjqueryui/css/smoothness/jquery-ui.css'
        'assets/bower_components/bootstrap-star-rating/css/star-rating.min.css'
        'assets/bower_components/fancybox/source/jquery.fancybox.css'
        'assets/bower_components/fancybox/source/helpers/jquery.fancybox-buttons.css'
        'assets/bower_components/fancybox/source/helpers/jquery.fancybox-thumbs.css'
        'assets/bower_components/multiple-select/multiple-select.css'
        'assets/bower_components/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.min.css'
        filter='cssrewrite' output='css/compiled/all.css' %}
        <link rel="stylesheet" href="{{ asset_url }}"/>

        {% endstylesheets %}

    {% endblock head_style %}

    <!-- Styles -->

    <!-- Favicon -->
    <link rel="shortcut icon" href="#">
</head>

<!-- Add class "boxed" along with body for boxed layout. -->
<!-- Add "pattern-x" (1 to 5) for background patterns. -->
<!-- Add "img-x" (1 to 5) for background images. -->
<body class="{% block body_class %}{% endblock %} {% if is_granted('ROLE_ADMIN') %}sf-toolbar{% endif %}">

{% include 'layout/loading.html.twig' %}

<!-- Outer Starts -->
<div class="outer">

    {% block outer %}

        <!-- Top bar starts -->
        {% include "layout/top-bar.html.twig" %}

        <!-- Top bar ends -->

        <!-- Header One Starts -->
        {% include "layout/header.html.twig" %}

        <!-- Header one ends -->

        {% block page_heading %}
            <div class="page-heading-two">
                <div class="container">
                    {% block headline_container %}
                        <h2>{% block headline %}Titre de la page{% endblock headline %}
                            {% block subtitle_container %}
                                <span>{% block subtitle %}{#Something can go here#}{% endblock subtitle %}</span>
                            {% endblock subtitle_container %}
                        </h2>
                    {% endblock headline_container %}
                    {% block breadcrumb_container %}
                        <div class="breads">
                            <a href="{{ path('jdj_web_homepage') }}">Accueil</a> /
                            {% block breadcrumb_content %}
                                {#<a href="#">Page</a> / #}
                                {#<a href="#">Sub Page</a>#}
                            {% endblock breadcrumb_content %}
                        </div>
                    {% endblock breadcrumb_container %}
                    <div class="clearfix"></div>
                </div>
            </div>
        {% endblock page_heading %}

        <!-- Main content starts -->

        <div class="main-block">

            <div class="magazine">

                {% include "layout/_flashes.html.twig" %}
                {% include "layout/_notify.html.twig" %}

                {% include "layout/login-form-modal.html.twig" %}

                {% block full_content %}

                    <div class="container">

                        <div class="row">

                            {% block fos_user_content %}
                                {% block body %}
                                    {% block container %}
                                        <!--
                                        Inherit this block to write
                                        Content without sidebar
                                        -->
                                        <div class="col-md-8">

                                            {% block container_content %}
                                                <!--
                                                Inherit this block to write
                                                Content with a sidebar
                                                -->
                                            {% endblock container_content %}

                                        </div>

                                        <!-- Sidebar column -->
                                        <div class="col-md-4">

                                            <!-- Sidebar -->
                                            <div class="sidebar">

                                                {% block sidebar_content %}
                                                    <!--
                                                    Inherit this block to write
                                                    Content in the sidebar
                                                    -->
                                                {% endblock sidebar_content %}

                                            </div>

                                        </div>

                                    {% endblock container %}
                                {% endblock body %}
                            {% endblock fos_user_content %}

                        </div>

                    </div>

                {% endblock full_content %}

            </div>

            <!-- Main content ends -->

        </div>

        {# Confirmation modal #}
        {% include '_confirm_modal.html.twig' %}

        <!-- Foot Starts -->

        {% include "layout/foot.html.twig" %}

        <!-- Foot Ends -->

        <!-- Footer Starts -->

        {% include "layout/footer.html.twig" %}

        <!-- Footer Ends -->

    {% endblock outer %}

</div>

<!-- Outer Ends -->

<!-- Scroll to top -->
<span class="totop"><a href="#"><i class="fa fa-angle-up bg-color"></i></a></span>

<!-- Javascript files -->

{% block foot_script_assetic %}
    {% javascripts output='js/compiled/all.js'
    'bundles/bmatznerjquery/js/jquery.min.js'
    'bundles/bmatznerjqueryui/js/minified/jquery-ui.min.js'
    '@MopaBootstrapBundle/Resources/public/bootstrap-sass/assets/javascripts/bootstrap/tooltip.js'
    '@MopaBootstrapBundle/Resources/public/bootstrap-sass/assets/javascripts/bootstrap/*.js'
    '@MopaBootstrapBundle/Resources/public/js/mopabootstrap-collection.js'
    '@MopaBootstrapBundle/Resources/public/js/mopabootstrap-subnav.js'
    'assets/bower_components/bootstrap-star-rating/js/star-rating.min.js'
    'assets/bower_components/fancybox/lib/jquery.mousewheel-3.0.6.pack.js'
    'assets/bower_components/fancybox/source/jquery.fancybox.pack.js'
    'assets/bower_components/fancybox/source/helpers/jquery.fancybox-buttons.js'
    'assets/bower_components/fancybox/source/helpers/jquery.fancybox-media.js'
    'assets/bower_components/fancybox/source/helpers/jquery.fancybox-thumbs.js'
    'assets/bower_components/multiple-select/jquery.multiple.select.js'
    'assets/bower_components/magnific-popup/dist/jquery.magnific-popup.min.js'
    'assets/bower_components/moment/min/moment.min.js'
    'assets/bower_components/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js'
    'assets/js/includes/*.js'
    'assets/js/*.js'
    'assets/js/modules/*/*'
    '@JDJJeuBundle/Resources/public/js/jeu/*' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
{% endblock foot_script_assetic %}

{% render(controller(
"cmf_create.jsloader.controller:includeJSFilesAction",
{'_locale': app.request.locale}
)) %}

</body>
</html>