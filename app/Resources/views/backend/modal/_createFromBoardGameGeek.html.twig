<div class="ui modal" id="board-game-geek-modal">
    <i class="close icon"></i>
    <div class="header">
        {{ 'app.ui.from_board_game_geek'|trans }}
    </div>
    <div class="image content">
        <div class="ui medium image">
            <img src="data:image/jpeg;base64,">
        </div>
        <div class="description">
            <div class="ui header">Importer un jeu depuis BoardGameGeek</div>
            <form action="{{ path('sylius_admin_product_from_bgg_new') }}" class="ui form">
                <div class="ui basic segment">
                    <div class="field">
                        <div class="ui labeled input">
                            <div class="ui label">
                                {{ 'app.ui.url'|trans }}
                            </div>
                            <input required type="text" name="bggPath"
                                   placeholder="https://boardgamegeek.com/boardgame/3076/puerto-rico">
                        </div>
                    </div>
                </div>

                <div class="ui basic segment">
                    <div class="ui buttons">
                        <button class="ui blue labeled icon button">
                            <i class="checkmark icon"></i>
                            {{ 'app.ui.submit'|trans }}
                        </button>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>