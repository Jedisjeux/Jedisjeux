<div class="ui tab" data-tab="attributes">
    <h3 class="ui dividing header">{{ 'sylius.ui.attributes'|trans }}</h3>

    {{ form_row(form.ageMin) }}

    <h4 class="ui dividing header">{{ 'label.release_date'|trans }}</h4>
    <div class="two fields">
        {{ form_row(form.firstVariant.releasedAtPrecision) }}
        {{ form_row(form.firstVariant.releasedAt) }}
    </div>

    <h4 class="ui dividing header">{{ 'label.player_count'|trans }}</h4>
    <div class="two fields">
        {{ form_row(form.joueurMin) }}
        {{ form_row(form.joueurMax) }}
    </div>

    <h4 class="ui dividing header">{{ 'label.duration'|trans }}</h4>
    <div class="two fields">
        {{ form_row(form.durationMin) }}
        {{ form_row(form.durationMax) }}
    </div>
</div>
