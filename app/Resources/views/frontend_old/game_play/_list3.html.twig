{% from 'macros/misc.html.twig' import no_data %}

{% set game_plays = game_plays.data is defined ? game_plays.data : game_plays %}

{% if game_plays|length %}
    <div class="img-box-4 text-center jeu-list">
        <div class="row">
            {% for key, game_play in game_plays %}
            {% if key != 0 and key%3 == 0 %}
        </div>
        <div class="row">
            {% endif %}
            <div class="col-md-4 col-sm-6">
                {% include "frontend/game_play/_singleBox.html.twig" %}
            </div>
            {% endfor %}
        </div>
    </div>
{% else %}
    {{ no_data('Aucune partie') }}
{% endif %}