{
    "timeout": 25,
    "source": {
        "directories": [
            "src"
        ],
        "excludes": [
            "Behat",
            "Command",
            "DependencyInjection",
            "Formatter",
            "Menu",
            "Migrations",
            "Repository",
            "Fixture",
            "Form/Filter",
            "Form/Type",
            "View",
            "Kernel.php"
        ]
    },
    "logs": {
        "text": "infection-log.txt"
    },
    "mutators": {
        "@default": true,
        "IdenticalEqual": false,
        "NotIdenticalNotEqual": false
    },
    "testFramework":"phpspec"
}
