{
  "name": "jedisjeux/jedisjeux",
  "description": "Boardgames website, based on Symfony framework.",
  "license": "MIT",
  "type": "project",
  "autoload": {
    "psr-4": {
      "AppBundle\\": "src/AppBundle/"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^5.5.9|^7.0",
    "symfony/symfony": "3.3.*",
    "doctrine/orm": "^2.4.8",
    "doctrine/doctrine-bundle": "~1.4",
    "doctrine/doctrine-fixtures-bundle": " ^2.2",
    "doctrine/doctrine-migrations-bundle": "^1.1",
    "knplabs/doctrine-behaviors": "~1.1",
    "symfony/assetic-bundle": "~2.3",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/monolog-bundle": "~2.4",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "incenteev/composer-parameter-handler": "~2.0",
    "guzzlehttp/guzzle": "^6.2",
    "eightpoints/guzzle-bundle": "^4.5",
    "friendsofsymfony/jsrouting-bundle": "^1.6.2",
    "incenteev/hashed-asset-bundle": "^1.0",
    "symfony-cmf/block-bundle": "^2.0",
    "symfony-cmf/media-bundle": "^1.3",
    "sylius/user-bundle": "^1.0.0",
    "sylius/customer-bundle": "^1.0.0",
    "sylius/fixtures-bundle": "^1.0.0",
    "sylius/grid-bundle": "^1.0.0",
    "sylius/locale-bundle": "^1.0.0",
    "sylius/mailer-bundle": "^1.0.0",
    "sylius/review-bundle": "^1.0.0",
    "sylius/product-bundle": "^1.0.0",
    "sylius/resource-bundle": "^1.0.0",
    "sylius/taxonomy-bundle": "^1.0.0",
    "sylius/ui-bundle": "^1.0.0",
    "winzou/state-machine-bundle": "^0.3",
    "liip/imagine-bundle": "^1.6",
    "jms/translation-bundle": "^1.3",
    "jms/di-extra-bundle": "^1.8",
    "friendsofsymfony/elastica-bundle": "^4.0",
    "twbs/bootstrap-sass": "^3.3",
    "mopa/bootstrap-bundle": "^3.1",
    "egeloen/ckeditor-bundle": "^4.0",
    "bmatzner/jquery-ui-bundle": "^1.10",
    "fzaninotto/Faker": "^1.5",
    "bmatzner/fontawesome-bundle": "^4.6",
    "infinite-networks/form-bundle": "^1.0",
    "google/apiclient": "^2.0",
    "zenstruck/redirect-bundle": "^3.0",
    "symfony-cmf/routing-bundle": "^2.0",
    "sylius/money-bundle": "^1.0.0",
    "eko/feedbundle": "^1.2",
    "ruflin/elastica": "^3.2"
  },
  "require-dev": {
    "behat/behat": "~3.0",
    "behat/symfony2-extension": "~2.0@dev",
    "behat/mink-extension": "~2.0@dev",
    "behat/mink-browserkit-driver": "~1.2@dev",
    "behat/mink-selenium2-driver": "@dev",
    "behat/mink": "~1.6@dev",
    "se/selenium-server-standalone": " ^2.52",
    "friends-of-behat/context-service-extension": "^0.3",
    "friends-of-behat/cross-container-extension": "^0.2",
    "friends-of-behat/performance-extension": "^1.0",
    "friends-of-behat/service-container-extension": "^0.3",
    "friends-of-behat/symfony-extension": "^0.2.1",
    "friends-of-behat/variadic-extension": "^0.1",
    "phpspec/phpspec": "^3.1",
    "lbaey/chromedriver": "^1.0",
    "lakion/api-test-case": "^1.1",
    "malukenho/kawaii-gherkin": "^0.1.3"
  },
  "scripts": {
    "symfony-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "@symfony-scripts"
    ],
    "post-update-cmd": [
      "@symfony-scripts"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "branch-alias": null
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
