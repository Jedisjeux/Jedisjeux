<div class="ui tab" data-tab="attributes">
    <h3 class="ui dividing header">{{ 'sylius.ui.attributes'|trans }}</h3>

    {{ form_row(form.minAge) }}

    <h4 class="ui dividing header">{{ 'app.ui.release_date'|trans }}</h4>
    <div class="two fields">
        {{ form_row(form.firstVariant.releasedAtPrecision) }}
        {{ form_row(form.firstVariant.releasedAt) }}
    </div>

    <h4 class="ui dividing header">{{ 'app.ui.player_count'|trans }}</h4>
    <div class="two fields">
        {{ form_row(form.minPlayerCount) }}
        {{ form_row(form.maxPlayerCount) }}
    </div>

    <h4 class="ui dividing header">{{ 'app.ui.duration'|trans }}</h4>
    <div class="two fields">
        {{ form_row(form.minDuration) }}
        {{ form_row(form.maxDuration) }}
    </div>
</div>
