{% for child in taxons %}
    <li class="nav-item">
        <a class="nav-link{% if current is not null and current.slug == child.slug %} active{% endif %}"
           href="{{ path('app_frontend_topic_index_by_taxon', { 'slug': child.slug }) }}">
            {% if child.iconClass is not null %}
                <i class="{{ child.iconClass }} pr-2"></i>
            {% endif %}
            {{ child.name }}
        </a>
    </li>
{% endfor %}
